<?xml version="1.0" encoding="UTF-8"?>
<localEntry key="LogstashHTTP" xmlns="http://ws.apache.org/ns/synapse">
  <http.init>
    <connectionType>HTTP</connectionType>
    <baseUrl>{${configs.logstash_http_endpoint}}</baseUrl>
    <authType>None</authType>
    <timeoutAction>Never</timeoutAction>
    <retryCount>0</retryCount>
    <retryDelay>0</retryDelay>
    <suspendErrorCodes>0</suspendErrorCodes>
    <suspendInitialDuration>0</suspendInitialDuration>
    <suspendProgressionFactor>1</suspendProgressionFactor>
    <suspendMaximumDuration>0</suspendMaximumDuration>
    <name>LogstashHTTP</name>
  </http.init>
</localEntry>